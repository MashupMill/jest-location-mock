{
	"name": "jest-location-mock",
	"version": "1.0.0",
	"description": "Jest hooks for JSDOM location mock",
	"keywords": [
		"jest",
		"jsdom",
		"location"
	],
	"homepage": "https://github.com/evelynhathaway/jest-location-mock#readme",
	"bugs": {
		"url": "https://github.com/evelynhathaway/boilerplate/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/evelynhathaway/jest-location-mock.git"
	},
	"license": "MIT",
	"author": "Evelyn Hathaway <npm@evelyn.dev> (https://evelyn.dev)",
	"sideEffects": true,
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"directories": {
		"lib": "/lib",
		"src": "/src"
	},
	"files": [
		"/lib"
	],
	"scripts": {
		"build": "tsc -p ./tsconfig-build.json",
		"lint": "eslint ./",
		"test": "jest"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"commitlint": {
		"extends": "./node_modules/conventional-changelog-evelyn/commitlint.config.js"
	},
	"lint-staged": {
		"**/package.json": "sort-package-json",
		"./": "eslint --fix"
	},
	"config": {
		"commitizen": {
			"path": "@commitlint/prompt"
		}
	},
	"dependencies": {
		"jest-diff": "^26.6.2"
	},
	"devDependencies": {
		"@commitlint/prompt": "^11.0.0",
		"@jedmao/location": "^3.0.0",
		"@semantic-release/changelog": "^5.0.1",
		"@semantic-release/git": "^9.0.0",
		"@typescript-eslint/eslint-plugin": "^4.8.1",
		"@typescript-eslint/parser": "^4.5.0",
		"commitlint": "^11.0.0",
		"conventional-changelog-evelyn": "^1.2.1",
		"eslint": "^7.14.0",
		"eslint-plugin-evelyn": "^4.0.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-regexp": "^0.1.1",
		"eslint-plugin-testing-library": "^3.10.1",
		"eslint-plugin-unicorn": "^23.0.0",
		"husky": "^4.3.0",
		"jest": "^26.6.3",
		"lint-staged": "^10.5.1",
		"semantic-release": "^17.2.4",
		"sort-package-json": "^1.48.0",
		"ts-jest": "^26.4.4",
		"typescript": "^4.1.2"
	},
	"engines": {
		"node": "^10.0.0"
	},
	"release": {
		"extends": "conventional-changelog-evelyn/release.config.js"
	}
}
